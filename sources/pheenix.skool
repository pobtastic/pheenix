; Check Collision
;
; Used by the routines at #R$6DCF, #R$6F43 and #R$6FDD.
@label=CheckCollision
c$6C9D LD A,($6693)  ; {Return if #R$6693 is active.
 $6CA0 OR A          ;
 $6CA1 RET NZ        ; }
 $6CA2 LD HL,($66ED) ; Load the player attribute position from #R$66ED.
 $6CA5 LD B,$02      ; Set a counter in #REGb for #N$02 rows to check.
@label=CheckCollision_RowLoop
*$6CA7 PUSH BC       ; {Stash the row counter and attribute position on the
 $6CA8 PUSH HL       ; stack.}
 $6CA9 LD B,$03      ; Set a counter in #REGb for #N$03 columns to check.
@label=CheckCollision_ColumnLoop
*$6CAB LD A,(HL)     ; Load the attribute at this position.
 $6CAC OR A          ; {Skip this position if the attribute is #COLOUR$00
 $6CAD JR Z,$6CBF    ; (empty space).}
 $6CAF CP $46        ; {Skip this position if the attribute is #COLOUR$46
 $6CB1 JR Z,$6CBF    ; (player ship).}
 $6CB3 CP $07        ; {Skip this position if the attribute is #COLOUR$07 (white/
 $6CB5 JR Z,$6CBF    ; background).}
; See #POKE#immunity(Immunity).
; .
; Collision detected - trigger player death.
 $6CB7 LD A,$01      ; {Set #R$66A4 to #N$01.
 $6CB9 LD ($66A4),A  ; }
 $6CBC POP AF        ; {Discard the saved positions from the stack.
 $6CBD POP AF        ; }
 $6CBE RET           ; Return.
; Move to the next attribute.
@label=CheckCollision_NextColumn
*$6CBF INC HL        ; Move to the next attribute position (right one column).
 $6CC0 DJNZ $6CAB    ; Loop back to #R$6CAB until all #N$03 columns are
                     ; checked.
 $6CC2 POP HL        ; Restore the starting attribute position for this row.
 $6CC3 SET 5,L       ; Move down one row (set bit 5 of #REGl = add #N$20).
 $6CC5 POP BC        ; {Restore the row counter and loop back to #R$6CA7 until both rows
 $6CC6 DJNZ $6CA7    ; are checked.}
 $6CC8 RET           ; Return.

; Draw Ship
;
; Used by the routines at #R$72C9 and #R$740C.
; .
; #PUSHS #POKES$66F3,$00;$74EF,$00;$74F0,$00;$74F1,$00
; #CLS($05) #UDGTABLE {
;   #SIM(start=$72C9,stop=$6FDB)#SCR$02(ship)
; } TABLE# #POPS
@label=DrawShip
c$6CC9 LD HL,$604E   ; Point #REGhl to the ship graphic: #R$604E.
 $6CCC LD DE,$50CF   ; Set the screen buffer location #N$50CF.
 $6CCF LD B,$02      ; Set a counter in #REGb for #N$02 rows.
@label=DrawShip_RowLoop
*$6CD1 PUSH BC       ; Stash the row counter on the stack.
 $6CD2 LD B,$02      ; And a counter in #REGc for #N$02 columns.
@label=DrawShip_ColumnLoop
*$6CD4 PUSH BC       ; {Stash the column counter and the screen position on the
 $6CD5 PUSH DE       ; stack.}
 $6CD6 LD B,$08      ; Set a line counter in #REGb (#N$08 lines in a UDG).
@label=DrawShip_LineLoop
*$6CD8 LD A,(HL)     ; {Copy the UDG data to the screen buffer.
 $6CD9 LD (DE),A     ; }
 $6CDA INC HL        ; Move to the next UDG graphic data byte.
 $6CDB INC D         ; Move down one pixel line in the screen buffer.
 $6CDC DJNZ $6CD8    ; Decrease the line counter by one and loop back to #R$6CD8 until all
                     ; #N$08 lines of the UDG character have been drawn.
 $6CDE POP DE        ; Restore the starting screen position from the stack.
 $6CDF INC DE        ; Increment #REGde by one.
 $6CE0 POP BC        ; Restore the column counter from the stack.
 $6CE1 DJNZ $6CD4    ; Decrease the column counter by one and loop back to #R$6CD4 until
                     ; both columns have been drawn to the screen buffer.
 $6CE3 LD DE,$50EF   ; Update the screen buffer pointer in #REGde to #N$50EF.
 $6CE6 POP BC        ; Restore the row counter from the stack.
 $6CE7 DJNZ $6CD1    ; Decrease the row counter by one and loop back to #R$6CD1 until both
                     ; rows have been drawn to the screen buffer.
 $6CE9 LD L,$CE      ; #REGl=#N$CE.
 $6CEB JP $6F93      ; Jump to #R$6F93.

; Handler: Player Death/ Game Over
;
; Used by the routines at #R$7C82 and #R$7DD9.
@label=Handler_PlayerDeathGameOver
c$6CEE LD HL,$66A4   ; {Jump to #R$6DCF if *#R$66A4 is zero.
 $6CF1 LD A,(HL)     ;
 $6CF2 OR A          ;
 $6CF3 JP Z,$6DCF    ; }
 $6CF6 INC HL        ; {Jump to #R$6D00 if *#R$66A5 is not equal to #N$40.
 $6CF7 LD A,(HL)     ;
 $6CF8 CP $40        ;
 $6CFA JR NZ,$6D00   ; }
 $6CFC DEC HL        ; {Write #N$02 ("explosion complete") to *#R$66A4.
 $6CFD LD (HL),$02   ; }
 $6CFF RET           ; Return.
; Handles the "Game Over" state.
@label=GameOver
*$6D00 LD A,($6695)  ; {Jump to #R$7030 if *#R$6695 is not zero.
 $6D03 OR A          ;
 $6D04 JP NZ,$7030   ; }
 $6D07 LD A,($66F0)  ; {Jump to #R$6D29 if *#R$66F0 is not equal to #N$01.
 $6D0A CP $01        ;
 $6D0C JR NZ,$6D29   ; }
 $6D0E LD A,($66F3)  ; {Jump to #R$6D29 if *#R$66F3 is active.
 $6D11 OR A          ;
 $6D12 JR NZ,$6D29   ; }
 $6D14 PUSH HL       ; Stash #REGhl on the stack.
; Prints #FONT#(:(#STR($646E,$03,$04)))$3D00,attr=$45(game)
 $6D15 LD HL,$646E   ; #REGhl=#R$646E.
 $6D18 LD DE,$48CB   ; #REGde=#N$48CB (screen buffer location).
 $6D1B LD BC,$0445   ; Set to print #N$04 characters in #COLOUR$45.
 $6D1E CALL $676F    ; Call #R$676F.
; Prints #FONT#(:(#STR($6472,$03,$04)))$3D00,attr=$45(over)
 $6D21 LD E,$D1      ; Update the screen buffer address.
 $6D23 LD B,$04      ; Set to print #N$04 characters.
 $6D25 CALL $676F    ; Call #R$676F.
 $6D28 POP HL        ; Restore #REGhl from the stack.
; Animate the explosion effect.
@label=GameOver_Explosion
*$6D29 LD A,(HL)     ; #REGa=*#REGhl.
 $6D2A AND %00000111 ; Keep only bits 0-2.
 $6D2C BIT 3,(HL)    ; {Jump to #R$6D31 if bit 3 of *#REGhl is set.
 $6D2E JR NZ,$6D31   ; }
 $6D30 INC A         ; Increment #REGa by one.
@label=GameOver_SkipIncrement
*$6D31 INC (HL)      ; {Increment the explosion counter by two.
 $6D32 INC (HL)      ; }
 $6D33 LD C,A        ; Copy the attribute value to #REGc.
 $6D34 LD B,$00      ; #REGb=#N$00.
 $6D36 LD IX,$0002   ; #REGix=#N$0002.
@label=GameOver_ExplosionLoop
*$6D3A PUSH BC       ; Stash the colour counter on the stack.
 $6D3B LD HL,($66ED) ; #REGhl=*#R$66ED.
 $6D3E LD DE,$0021   ; {#REGhl+=#N($0021,$04,$04).
 $6D41 ADD HL,DE     ; }
 $6D42 LD (HL),D     ; Write #REGd to *#REGhl.
 $6D43 PUSH HL       ; Stash #REGhl on the stack.
 $6D44 LD A,L        ; #REGa=#REGl.
 $6D45 AND %00011111 ; Keep only bits 0-4.
 $6D47 SUB C         ; #REGa-=#REGc.
 $6D48 PUSH AF       ; Stash #REGaf on the stack.
 $6D49 AND A         ; {#REGhl-=#REGbc.
 $6D4A SBC HL,BC     ; }
 $6D4C POP AF        ; Restore #REGaf from the stack.
 $6D4D CALL NC,$6926 ; Call #R$6926 if #REGa is greater than #N$00.
 $6D50 POP HL        ; Restore #REGhl from the stack.
 $6D51 PUSH HL       ; Stash #REGhl on the stack.
 $6D52 LD A,L        ; #REGa=#REGl.
 $6D53 AND %00011111 ; Keep only bits 0-4.
 $6D55 ADD A,C       ; #REGa+=#REGc.
 $6D56 CP $20        ; Compare #REGa with #N$20.
 $6D58 PUSH AF       ; Stash #REGaf on the stack.
 $6D59 ADD HL,BC     ; #REGhl+=#REGbc.
 $6D5A POP AF        ; Restore #REGaf from the stack.
 $6D5B CALL C,$6926  ; Call #R$6926 if #REGa is less than #N$20.
 $6D5E POP HL        ; Restore #REGhl from the stack.
 $6D5F PUSH HL       ; Stash #REGhl on the stack.
 $6D60 LD B,C        ; #REGb=#REGc.
 $6D61 LD DE,$0020   ; {#REGhl-=#N($0020,$04,$04) (with carry).
*$6D64 AND A         ;
 $6D65 SBC HL,DE     ; }
 $6D67 DJNZ $6D64    ; Decrease counter by one and loop back to #R$6D64 until
                     ; counter is zero.
 $6D69 CALL $6926    ; Call #R$6926.
 $6D6C POP HL        ; Restore #REGhl from the stack.
 $6D6D LD A,C        ; #REGa=#REGc.
 $6D6E SUB $02       ; #REGa-=#N$02.
 $6D70 LD B,A        ; #REGb=#REGa.
 $6D71 JR C,$6D8A    ; Jump to #R$6D8A if #REGa is less than #REGa.
 $6D73 JR Z,$6D8A    ; Jump to #R$6D8A if #REGa is zero.
 $6D75 PUSH HL       ; Stash #REGhl on the stack.
 $6D76 LD DE,$0021   ; #REGde=#N($0021,$04,$04).
 $6D79 LD A,L        ; #REGa=#REGl.
 $6D7A AND %00011111 ; Keep only bits 0-4.
 $6D7C ADD A,$02     ; #REGa+=#N$02.
 $6D7E SUB C         ; #REGa-=#REGc.
 $6D7F PUSH AF       ; Stash #REGaf on the stack.
*$6D80 AND A         ; {#REGhl-=#REGde (with carry).
 $6D81 SBC HL,DE     ; }
 $6D83 DJNZ $6D80    ; Decrease counter by one and loop back to #R$6D80 until
                     ; counter is zero.
 $6D85 POP AF        ; Restore #REGaf from the stack.
 $6D86 CALL NC,$6926 ; Call #R$6926 if #REGa is greater than #N$00.
 $6D89 POP HL        ; Restore #REGhl from the stack.
*$6D8A LD A,C        ; #REGa=#REGc.
 $6D8B SUB $02       ; #REGa-=#N$02.
 $6D8D LD B,A        ; #REGb=#REGa.
 $6D8E JR C,$6DA4    ; Jump to #R$6DA4 if #REGa is less than #REGa.
 $6D90 JR Z,$6DA4    ; Jump to #R$6DA4 if #REGa is zero.
 $6D92 LD DE,$001F   ; #REGde=#N($001F,$04,$04).
 $6D95 LD A,L        ; #REGa=#REGl.
 $6D96 AND E         ; Merge the bits from #REGe.
 $6D97 ADD A,C       ; #REGa+=#REGc.
 $6D98 CP $22        ; Compare #REGa with #N$22.
 $6D9A PUSH AF       ; Stash #REGaf on the stack.
*$6D9B AND A         ; {#REGhl-=#REGde (with carry).
 $6D9C SBC HL,DE     ; }
 $6D9E DJNZ $6D9B    ; Decrease counter by one and loop back to #R$6D9B until
                     ; counter is zero.
 $6DA0 POP AF        ; Restore #REGaf from the stack.
 $6DA1 CALL C,$6926  ; Call #R$6926 if #REGa is less than #N$00.
*$6DA4 POP BC        ; Restore #REGbc from the stack.
 $6DA5 INC C         ; Increment #REGc by one.
 $6DA6 DEC IXl       ;
 $6DA8 JR NZ,$6D3A   ; Jump to #R$6D3A if ?? is not equal to #N$00.
 $6DAA LD A,($66F3)  ; {Jump to #R$7030 if *#R$66F3 is not active.
 $6DAD OR A          ;
 $6DAE JP NZ,$7030   ; }
 $6DB1 LD A,($66A5)  ; #REGa=*#R$66A5.
 $6DB4 AND %00000011 ; Keep only bits 0-1.
 $6DB6 JP NZ,$7030   ; Jump to #R$7030 if the result is not equal to zero.
 $6DB9 LD B,$08      ; Set sound loop counter to #N$08.
@label=GameOver_SoundLoop
*$6DBB LD A,B        ; #REGa=#REGb.
 $6DBC ADD A,$19     ; #REGa+=#N$19.
 $6DBE AND %00011000 ; Keep only bits 3-4.
 $6DC0 DI            ; Disable interrupts.
 $6DC1 OUT ($FE),A   ; Set border to the colour held by #REGa.
 $6DC3 PUSH BC       ; Stash #REGbc on the stack.
@label=GameOver_SoundDelay
*$6DC4 DJNZ $6DC4    ; Decrease the delay loop counter by one and loop back to #R$6DC4
                     ; until the counter is zero.
 $6DC6 POP BC        ; Restore #REGbc from the stack.
 $6DC7 DJNZ $6DBB    ; Decrease counter by one and loop back to #R$6DBB until
                     ; counter is zero.
 $6DC9 XOR A         ; {Turn the speaker off.
 $6DCA OUT ($FE),A   ; }
 $6DCC JP $7030      ; Jump to #R$7030.

; Handler: Shield
;
; Used by the routine at #R$6CEE.
@label=Handler_Shield
c$6DCF CALL $6C9D    ; Call #R$6C9D.
 $6DD2 LD A,($66ED)  ; #REGa=*#R$66ED.
 $6DD5 PUSH AF       ; Stash the player position on the stack.
 $6DD6 LD HL,$6694   ; {Jump to #R$6DE8 if *#R$6694 isn't active.
 $6DD9 LD A,(HL)     ;
 $6DDA OR A          ;
 $6DDB JR Z,$6DE8    ; }
; The players shield is active, so decrease the shield timer.
 $6DDD DEC (HL)      ; Decrease *#R$6694 by one.
; Check if the timer has "wrapped around" to #N$FF i.e. finished counting
; down.
 $6DDE BIT 7,(HL)    ; {Jump to #R$6E20 if bit 7 of *#R$6694 is set.
 $6DE0 JR NZ,$6E20   ; }
 $6DE2 XOR A         ; {Write #N$00 ("inactive") to *#R$6693.
 $6DE3 LD ($6693),A  ; }
 $6DE6 JR $6E20      ; Jump to #R$6E20.
; Check for shield activation player inputs.
; .
; Start by checking if the game is running in demo mode or not?
@label=Shield_CheckDemoMode
*$6DE8 LD A,($66F3)  ; {Jump to #R$6DF6 if *#R$66F3 is not active.
 $6DEB OR A          ;
 $6DEC JR Z,$6DF6    ; }
; Demo mode is active, so use a random number to decide whether to
; activate the shield or not.
 $6DEE CALL $670E    ; Call #R$670E.
 $6DF1 OR A          ; {Jump to #R$6E1B if the generated random number is zero.
 $6DF2 JR Z,$6E1B    ; }
 $6DF4 JR $6E20      ; Jump to #R$6E20.
; Check if the control method is the Kempston joystick?
@label=Shield_CheckKempston
*$6DF6 LD A,($66F6)  ; {Jump to #R$6E05 if *#R$66F6 is not the Kempston
 $6DF9 CP $02        ; joystick.
 $6DFB JR NZ,$6E05   ; }
; The control method is Kempston joystick, so test for "down" being
; pressed.
 $6DFD IN A,($1F)    ; #REGa=read from the Kempston joystick port.
 $6DFF AND %00000100 ; {Jump to #R$6E1B if "down" (bit 2) is being pressed.
 $6E01 JR NZ,$6E1B   ; }
 $6E03 JR $6E20      ; Jump to #R$6E20.
; Check if the control method is the AGF joystick?
@label=Shield_CheckAGF
*$6E05 CP $01        ; {Jump to #R$6E13 if *#R$66F6 is not the AGF joystick.
 $6E07 JR NZ,$6E13   ; }
; The control method is the AGF joystick, so test for "down" being
; pressed.
 $6E09 LD A,$EF      ; {Read from the keyboard;
 $6E0B IN A,($FE)    ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                     ; { =h,r2 Port Number | =h,c5 Bit }
                     ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                     ; { #N$EF | 0 | 9 | 8 | 7 | 6 }
                     ; TABLE#}
 $6E0D BIT 4,A       ; {Jump to #R$6E1B if "down" ("4") was pressed.
 $6E0F JR Z,$6E1B    ; }
 $6E11 JR $6E20      ; Jump to #R$6E20.
; Else, the only control option left is the keyboard.
@label=Shield_CheckKeyboard
*$6E13 LD A,$BF      ; {Read from the keyboard;
 $6E15 IN A,($FE)    ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                     ; { =h,r2 Port Number | =h,c5 Bit }
                     ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                     ; { #N$BF | ENTER | L | K | J | H }
                     ; TABLE#}
 $6E17 AND %00000001 ; {Jump to #R$6E20 if "ENTER" isn't being pressed.
 $6E19 JR NZ,$6E20   ; }
; Activate the shield.
@label=Shield_ActivateShield
*$6E1B LD (HL),$FF   ; Write #N$FF to *#R$6694.
 $6E1D DEC HL        ; {Write #N$01 to *#R$6693 to activate it.
 $6E1E LD (HL),$01   ; }
; Play a sound when the shield is active (but not in demo mode).
@label=PlayShieldSound
*$6E20 LD A,($6693)  ; {Jump to #R$6EF8 if *#R$6693 is not active.
 $6E23 OR A          ;
 $6E24 JP Z,$6EF8    ; }
 $6E27 LD A,($66F3)  ; {Jump to #R$6E40 if *#R$66F3 is active.
 $6E2A OR A          ;
 $6E2B JR NZ,$6E40   ; }
; Play an iteration of the shield sound effect.
; .
; #HTML(#AUDIO(shield.wav)(#INCLUDE(Shield)))
 $6E2D LD A,($6694)  ; #REGa=*#R$6694.
 $6E30 AND %00000111 ; {Calculate the sound pitch: #N($00DE,$04,$04) +
 $6E32 ADD A,A       ; ((*#R$6694 & #N$07)  #N$04).
 $6E33 ADD A,A       ;
 $6E34 LD HL,$00DE   ;
 $6E37 LD D,H        ;
 $6E38 LD E,A        ;
 $6E39 ADD HL,DE     ; }
 $6E3A LD E,$04      ; Set the duration in #REGe to #N$04.
 $6E3C CALL $03B5    ; #HTML(Call <a "noopener nofollow"
                     ; href="https://skoolkit.ca/disassemblies/rom/hex/asm/03B5.html">BEEPER</a>.)
 $6E3F DI            ; Disable interrupts.
; Draw shield graphic.
@label=DrawShield
*$6E40 LD HL,($66ED) ; #REGhl=*#R$66ED.
 $6E43 LD H,$52      ; #REGh=#N$52.
 $6E45 LD B,$02      ; #REGb=#N$02.
 $6E47 LD A,($66A6)  ; #REGa=*#R$66A6.
 $6E4A PUSH AF       ; Stash #REGaf on the stack.
*$6E4B OR A          ; Set the bits from #REGa.
 $6E4C LD DE,$0000   ; #REGde=#N($0000,$04,$04).
 $6E4F JR NZ,$6E62   ; Jump to #R$6E62 if #REGhl is not equal to #REGa.
 $6E51 LD A,($6694)  ; {Jump to #R$6E62 if bit 2 of *#R$6694 is not set.
 $6E54 BIT 2,A       ;
 $6E56 JR Z,$6E62    ; }
 $6E58 LD DE,$0136   ; #REGde=#N($0136,$04,$04).
 $6E5B LD A,B        ; {Jump to #R$6E62 if #REGb is not equal to #N$01.
 $6E5C CP $01        ;
 $6E5E JR NZ,$6E62   ; }
 $6E60 LD D,$80      ; #REGd=#N$80.
*$6E62 LD ($6E87),DE ; Write #REGde to *#R$6E87.
 $6E66 LD ($6E9A),DE ; Write #REGde to *#R$6E9A.
 $6E6A POP AF        ; Restore #REGaf from the stack.

 $670F PUSH BC       ; }
 $6710 LD HL,($65DB) ; #REGhl=*#R$65DB.
 $6713 LD B,(HL)     ; Fetch a byte from *#REGhl and store it in #REGb.
 $6714 INC HL        ; Increment the seed pointer by one.

 $6E6C PUSH BC       ; Stash #REGbc on the stack.
 $6E6D LD B,A        ; #REGb=#REGa.
 $6E6E PUSH HL       ; Stash #REGhl on the stack.
 $6E6F LD HL,$6E86   ; #REGhl=#R$6E85(#N$6E86).
 $6E72 LD A,($6694)  ; #REGa=*#R$6694.
 $6E75 BIT 2,A       ; Test bit 2 of #REGa.
 $6E77 LD A,$7E      ; #REGa=#N$7E.
 $6E79 JR Z,$6E7D    ; Jump to #R$6E7D if #REGa is equal to #N$7E.
 $6E7B ADD A,$40     ; #REGa+=#N$40.
*$6E7D ADD A,$08     ; #REGa+=#N$08.
 $6E7F DJNZ $6E7D    ; Decrease counter by one and loop back to #R$6E7D until
                     ; counter is zero.
 $6E81 LD (HL),A     ; Write #REGa to *#REGhl.
 $6E82 POP HL        ; Restore #REGhl from the stack.
 $6E83 LD B,$06      ; #REGb=#N$06.
*$6E85 SET 0,(HL)    ; Set bit 0 of *#REGhl.
 $6E87 LD (HL),$01   ; Write #N$01 to *#REGhl.
 $6E89 INC H         ; Increment #REGh by one.
 $6E8A DJNZ $6E85    ; Decrease counter by one and loop back to #R$6E85 until
                     ; counter is zero.
 $6E8C LD A,($6E86)  ; #REGa=*#R$6E85(#N$6E86).
 $6E8F LD ($6E99),A  ; Write #REGa to *#R$6E98(#N$6E99).
 $6E92 SET 5,L       ; Set bit 5 of #REGl.
 $6E94 LD H,$50      ; #REGh=#N$50.
 $6E96 LD B,$08      ; #REGb=#N$08.
*$6E98 SET 0,(HL)    ; Set bit 0 of *#REGhl.
 $6E9A LD (HL),$01   ; Write #N$01 to *#REGhl.
 $6E9C INC H         ; Increment #REGh by one.
 $6E9D DJNZ $6E98    ; Decrease counter by one and loop back to #R$6E98 until
                     ; counter is zero.
 $6E9F LD HL,($66ED) ; #REGhl=*#R$66ED.
 $6EA2 LD H,$52      ; #REGh=#N$52.
 $6EA4 INC L         ; {Increment #REGl by two.
 $6EA5 INC L         ; }
 $6EA6 POP BC        ; Restore #REGbc from the stack.
 $6EA7 LD A,($66A6)  ; #REGa=*#R$66A6.
 $6EAA PUSH AF       ; Stash #REGaf on the stack.
 $6EAB SUB $07       ; #REGa-=#N$07.
 $6EAD DJNZ $6E4B    ; Decrease counter by one and loop back to #R$6E4B until
                     ; counter is zero.
 $6EAF POP AF        ; Restore #REGaf from the stack.
 $6EB0 LD A,($6694)  ; #REGa=*#R$6694.
 $6EB3 BIT 2,A       ; Test bit 2 of #REGa.
 $6EB5 LD HL,$6014   ; #REGhl=#R$6014.
 $6EB8 JR Z,$6EBD    ; Jump to #R$6EBD if #REGl is equal to #N$52.
 $6EBA LD HL,$601D   ; #REGhl=#R$601D.
*$6EBD LD DE,$617B   ; #REGde=#R$617B.
 $6EC0 LD C,$09      ; #REGc=#N$09.
 $6EC2 LDIR          ; LDIR.
 $6EC4 LD A,($66A6)  ; {Jump to #R$6EE1 if *#R$66A6 is zero.
